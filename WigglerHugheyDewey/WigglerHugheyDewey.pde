Wiggler hughey, dewey;
public void setup()
{
  size(300, 300);
  hughey = new Wiggler();
  dewey = new Wiggler();
  hughey.setmyY(100);
  dewey.setmyX(100);
}
public void draw()
{
  background(197);
  System.out.println(dewey.getmyX());
  dewey.wiggle();
  hughey.wiggle();
  System.out.println(hughey.getmyY());
  line((int)hughey.getmyX(), (int)hughey.getmyY(),(int)dewey.getmyX(), (int)dewey.getmyY()); 
}
